{
  "extends": "eslint:recommended",
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2020
  },
  "env": {
    "browser": true,
    "es6": true,
    "es2020": true,
    "node": true
  },
  "rules": {
    "semi": 2,
    "no-process-env": 2,
    "no-cond-assign": 0,
    "no-redeclare": 0
  }
}
